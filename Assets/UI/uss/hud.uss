.top {
    flex-grow: 1;
    padding-top: 4px;
    padding-right: 4px;
    padding-bottom: 8px;
    padding-left: 4px;
    max-height: 10%;
    background-color: rgb(51, 51, 255);
    border-left-color: rgb(0, 0, 255);
    border-right-color: rgb(0, 0, 255);
    border-top-color: rgb(0, 0, 255);
    border-bottom-color: rgb(0, 0, 255);
    border-right-width: 4px;
    border-bottom-width: 8px;
    border-left-width: 4px;
    border-top-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-right-radius: 8px;
    border-bottom-left-radius: 8px;
    flex-direction: row;
    min-height: 10px;
    border-top-width: 4px;
}

.healthTotal {
    flex-grow: 1;
    background-image: url('project://database/Assets/Sprites/HUD/4k/healthTotal.png?fileID=2800000&guid=275b46d50af759b47832d9568eaba257&type=3#healthTotal');
    max-width: 33%;
}

.healthCurrent {
    flex-grow: 1;
    background-image: url('project://database/Assets/Sprites/HUD/4k/healthCurrent.png?fileID=2800000&guid=70065efd72023c444914aaedf47b8fe5&type=3#healthCurrent');
    transform-origin: left;
}

.portrait {
    background-image: var(--portrait);
    margin-right: auto;
    margin-left: auto;
    border-left-color: rgb(0, 0, 255);
    border-right-color: rgb(0, 0, 255);
    border-top-color: rgb(0, 0, 255);
    border-bottom-color: rgb(0, 0, 255);
    border-top-width: 4px;
    border-right-width: 2px;
    border-bottom-width: 2px;
    border-left-width: 2px;
    border-top-left-radius: 8px;
    border-top-right-radius: 8px;
    border-bottom-right-radius: 8px;
    border-bottom-left-radius: 8px;
    flex-grow: 1;
    min-width: 128px;
    max-width: 256px;
}

.inventory {
    background-image: url('project://database/Assets/Sprites/HUD/4k/inventory.png?fileID=2800000&guid=fedbba59cd323134a9a8fee5dd6756f1&type=3#inventory');
    flex-direction: row;
    width: 33%;
}

.slot {
    flex-grow: 1;
    margin-right: 2%;
    margin-left: 2%;
    margin-top: 2%;
    margin-bottom: 2%;
    min-width: 16px;
    min-height: 16px;
    width: 20%;
}

.slot-selected {
    flex-grow: 1;
    border-left-color: rgb(255, 153, 0);
    border-right-color: rgb(255, 153, 0);
    border-top-color: rgb(255, 153, 0);
    border-bottom-color: rgb(255, 153, 0);
    border-top-width: 2px;
    border-right-width: 2px;
    border-bottom-width: 2px;
    border-left-width: 2px;
    border-top-left-radius: 2px;
    border-top-right-radius: 2px;
    border-bottom-right-radius: 2px;
    border-bottom-left-radius: 2px;
}

.slot1 {
    background-image: var(--slot1);
}

.slot2 {
    background-image: var(--slot2);
}

.slot3 {
    background-image: var(--slot3);
}

.slot4 {
    background-image: var(--slot4);
}

.slot5 {
    background-image: var(--slot5);
}

.slot6 {
    background-image: var(--slot6);
}

.bottom {
    flex-grow: 1;
    flex-direction: row;
    height: 20%;
    background-color: rgb(51, 51, 255);
    border-left-color: rgb(0, 0, 255);
    border-right-color: rgb(0, 0, 255);
    border-top-color: rgb(0, 0, 255);
    border-bottom-color: rgb(0, 0, 255);
    border-top-width: 8px;
    border-right-width: 4px;
    border-left-width: 4px;
    border-top-left-radius: 8px;
    border-top-right-radius: 8px;
    padding-top: 2px;
    padding-right: 2px;
    padding-bottom: 2px;
    padding-left: 2px;
    width: 100%;
    position: absolute;
    top: 80%;
    border-bottom-width: 4px;
}

.sequence-input {
    flex-grow: 1;
    max-width: 33%;
}

.sequencers {
    min-height: 75%;
}

.sequence {
    flex-grow: 1;
    flex-direction: row;
    border-right-width: 1px;
    border-bottom-width: 1px;
    border-top-left-radius: 32px;
    border-top-right-radius: 32px;
    border-bottom-right-radius: 32px;
    border-bottom-left-radius: 32px;
    border-left-color: rgb(0, 0, 0);
    border-right-color: rgb(0, 0, 0);
    border-top-color: rgb(0, 0, 0);
    border-bottom-color: rgb(0, 0, 0);
    margin-top: 1px;
    min-height: 20%;
}

.sequence-neutral {
    background-color: rgb(170, 170, 170);
}

.sequence-fire {
    background-color: rgb(255, 17, 0);
}

.sequence-ice {
    flex-grow: 1;
    flex-direction: row;
    margin-top: 1px;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    background-color: rgb(0, 0, 255);
}

.sequence-earth {
    background-color: rgb(0, 113, 0);
}

.sequence-air {
    background-color: rgb(255, 255, 0);
}

